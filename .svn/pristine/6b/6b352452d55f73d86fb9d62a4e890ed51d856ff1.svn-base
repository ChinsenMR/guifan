/* 0823新增 */

/* 公告通告栏 */
.srcoll_view {
  position: absolute;
  top: 0rpx;
  height: 80rpx;
  font-size: 30rpx;
  white-space: nowrap;
  line-height: 80rpx;
  color: #ffb432;
  animation: notice 20s linear infinite;
}

@keyframes notice {
  0% {
    margin-left: 750rpx;
  }
  100% {
    margin-left: -1300rpx;
  }
}

.scroll_view_border {
  position: relative;
  width: 100%;
  background:#ffffcc;
  height: 80rpx;
  overflow: hidden;
}
/* 公告通告栏完 */

