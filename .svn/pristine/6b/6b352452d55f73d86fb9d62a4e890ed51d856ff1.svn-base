/* 商品 */

/* 小图模式 */

.list ul {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 20rpx auto;
  width: 95%;
  background-color: #f9f9f9;
}

.list ul li .nav {
  background-color: white;
  margin-bottom: 20rpx;
  border: 2rpx solid #e9e9e9;
  padding: 10rpx;
}

.list ul li {
  width: 48%;
  /* height: 450rpx; */
  box-sizing: border-box;
}

.list ul li image {
  width: 100%;
  height: 320rpx;
}

.list ul li .title {
  font-size: 26rpx;
  color: #333;
  margin: 5rpx 0 5rpx 18rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.list ul li .oandp {
  display: flex;
}

.list ul li .price {
  font-size: 32rpx;
  color: #ff5f00;
  display: flex;
  align-items: center;
}

.list ul li .oprice {
  font-size: 24rpx;
  color: #929292;
  flex: 1;
  margin: 6rpx 0 0 10rpx;
  text-decoration: line-through;
}

.list ul li .scount {
  font-size: 24rpx;
  margin-left: 18rpx;
}

.addmycard {
  width: 90%;
  padding: 10rpx 0rpx;
  box-sizing: border-box;
  font-size: 30rpx;
  color: #fff;
  display: none;
  background: #e0001b;
  text-align: center;
  margin-top: 20rpx;
  position: relative;
  left: 5%;
  border-radius: 10rpx;
}

