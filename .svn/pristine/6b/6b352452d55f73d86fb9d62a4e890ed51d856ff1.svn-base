.content {
  margin: 20rpx auto 20rpx;
}

.itemtest {
  position: relative;
  height: 70rpx;
  width: 90%;
  border: 2rpx solid #ededed;
  line-height: 66rpx;
  margin: 0 auto;
  box-sizing: border-box;
  color: #646464;
  font-size: 30rpx;
  padding: 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.img-box {
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
