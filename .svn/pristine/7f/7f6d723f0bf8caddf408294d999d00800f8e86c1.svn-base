.layout {
  background-color: #f4f4f4;
  height: 100%;
}

.content-ul {
  padding-top: 21rpx;
}

.content-li {
  height: 88rpx;
  padding: 0 27rpx;
  background-color: #fff;
  margin-bottom: 1rpx;
  color: #333;
  font-size: 30rpx;
}

.content-li image {
  width: 23rpx;
  margin-left: 10rpx;
}

.content-li input {
  font-size: 28rpx;
  color: #333;
  width: 352rpx;
  text-align: right;
}

.inputStyle {
  color: #999;
}

.btn {
  position: fixed;
  bottom: 95rpx;
  left: 29rpx;
  right: 29rpx;
  height: 88rpx;
  border-radius: 44rpx;
  color: #fff;
  font-size: 30rpx;
  background-color: #f66;
}


/* 提现协议弹窗 */
.win_wrap {
  width: 80%;
  height: 1000rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  /* background-color: #fff; */
  z-index: 10999;
  box-shadow: 10px 10px 5px #888888;
  border-radius: 10rpx;
  border: 1rpx solid #eee;
}

.win_style {
  width: 100%;
  background-color: #fff;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}

.win_main {
  width: 100%;
  height: 900rpx;
  background: rgba(0, 0, 0, .6);
  position: relative;
}

.search_btn {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  border: 1rpx solid #fff;
  padding: 5rpx;
  box-sizing: border-box;
}

.btn_img {
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-radius: 50%;
}

.btn_wrap {
  position: absolute;
  left: 25%;
  bottom: 0;
  font-size: 30rpx;
  color: #fff;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.zhezhao {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  /* background: rgba(244, 244, 244, .6); */
  z-index: 99;
}